!_TAG_FILE_FORMAT	2	/extended format; --format=1 will not append ;" to lines/
!_TAG_FILE_SORTED	1	/0=unsorted, 1=sorted, 2=foldcase/
!_TAG_PROGRAM_AUTHOR	Darren Hiebert	/dhiebert@users.sourceforge.net/
!_TAG_PROGRAM_NAME	Exuberant Ctags	//
!_TAG_PROGRAM_URL	http://ctags.sourceforge.net	/official site/
!_TAG_PROGRAM_VERSION	5.8	//
CC	Makefile	/^CC=gcc$/;"	m
CFLAGS	Makefile	/^CFLAGS=-std=c99 $(DBGFLAG) -Wall $(OPTFLAG) $(PROGOPTS)$/;"	m
DBGFLAG	Makefile	/^DBGFLAG=-ggdb $/;"	m
LDFLAGS	Makefile	/^LDFLAGS=$(DBGFLAG) $(OPTFLAG)$/;"	m
LDLIBS	Makefile	/^LDLIBS=-lreadline$/;"	m
OBJECTS	Makefile	/^OBJECTS=$(SOURCES:.c=.o)$/;"	m
OPTFLAG	Makefile	/^OPTFLAG=$/;"	m
PROGOPTS	Makefile	/^PROGOPTS=$/;"	m
SOURCES	Makefile	/^SOURCES=signals.c$/;"	m
TARGET	Makefile	/^TARGET=signals$/;"	m
_XOPEN_SOURCE	signals.c	1;"	d	file:
handler	signals.c	/^static void handler(int sig, siginfo_t *si, void *unused){$/;"	f	file:
main	signals.c	/^int main(int argc, char *argv[]){$/;"	f
